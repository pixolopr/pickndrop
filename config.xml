<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<widget id="com.ionicframework.pickndrop692805" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>pickndrop</name>
    <description>
        Mobile App for PickNdrop.ca
    </description>
    <author email="contactabhay2@gmail.com" href="http://ionicframework.com/">
        Ionic Framework Team
    </author>
    <icon src="img/logo.png" />
    <content src="index.html"/>
    <access origin="*" />
    <gap:splash src="img/splash.png" />

    <preference name="phonegap-version" />
    <preference name="permissions" value="none" />
    <preference name="fullscreen" value="false" />
    <preference name="orientation" value="portrait" />
    <preference name="android-installLocation" value="internalOnly" />
    <preference name="ErrorUrl" value="" />
    <preference name="BackgroundColor" value="0x000000" />
    <preference name="KeepRunning" value="true" />
    <preference name="splash-screen-duration" value="5000" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="LoadUrlTimeoutValue" value="200000" />
    
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="BackupWebStorage" value="none" />
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" onload="true" />
    </feature>
</widget>